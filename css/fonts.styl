@font-face {
	font-family: 'Ume UI Gothic O5';
	src: url('fonts/subset-Ume-UI-Gothic-O5.eot');
	src: url('fonts/subset-Ume-UI-Gothic-O5.eot?#iefix') format('embedded-opentype'),
		url('fonts/subset-Ume-UI-Gothic-O5.woff2') format('woff2'),
		url('fonts/subset-Ume-UI-Gothic-O5.woff') format('woff'),
		url('fonts/subset-Ume-UI-Gothic-O5.ttf') format('truetype');
	font-weight: normal;
	font-style: normal;
}

@font-face {
    font-family: 'Ume P Mincho Regular';
    src: url('fonts/ume-p-mincho-webfont.eot');
    src: url('fonts/ume-p-mincho-webfont.eot?#iefix') format('embedded-opentype'),
         url('fonts/ume-p-mincho-webfont.woff2') format('woff2'),
         url('fonts/ume-p-mincho-webfont.woff') format('woff'),
         url('fonts/ume-p-mincho-webfont.ttf') format('truetype'),
         url('fonts/ume-p-mincho-webfont.svg#ume_p_minchoregular') format('svg');
    font-weight: normal;
    font-style: normal;

}